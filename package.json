{
  "name": "devshare",
  "version": "0.0.2",
  "description": "Library to communicate with tessellate application building/managing service.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "src",
    "dist",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "lint": "standard src",
    "test": "mocha -R spec --compilers js:babel-core/register ./test/setup.js ./test/**/*.spec.js",
    "test:cov": "babel-node $(npm bin)/isparta cover --report lcov ./node_modules/mocha/bin/_mocha -- ./test --recursive",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/devshare.js --config webpack.config.development.js",
    "build:umd:min": "webpack -p src/index.js dist/devshare.min.js --config webpack.config.production.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "watch:umd": "npm run build:umd -- --stats --progress --colors --watch",
    "watch:lib": "npm run build:lib -- --stats --progress --colors --watch",
    "watch": "npm run watch:umd",
    "prepublish": "npm run clean && npm run test && npm run build",
    "upload": "node ./bin/upload",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:prepare && gitbook build -g kypertech/devshare",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:publish": "npm run docs:clean && npm run docs:build && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:kypertech/devshare gh-pages --force"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kypertech/devshare.git"
  },
  "keywords": [
    "kyper",
    "matter",
    "tessellate",
    "authentication",
    "user",
    "project management"
  ],
  "author": "Kyper Digital Inc. <kyper.dev@gmail.com>",
  "contributors": [
    {
      "name": "Scott Prue",
      "email": "<sprue.dev@gmail.com>"
    },
    {
      "name": "Mel van Londen",
      "email": "<melvanlonden@gmail.com>"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kypertech/devshare/issues"
  },
  "homepage": "https://github.com/kypertech/devshare",
  "dependencies": {
    "cookie": "0.2.3",
    "firebase": "2.4.1",
    "isomorphic-fetch": "2.2.1",
    "jszip": "2.5.0",
    "lodash": "4.6.1",
    "node-safe-filesaver": "0.1.0",
    "qs": "^6.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-lodash": "^2.0.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "chai-properties": "^1.2.1",
    "es6-promise": "^3.1.2",
    "gitbook-cli": "^2.1.2",
    "isparta": "^4.0.0",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "nock": "^7.2.2",
    "rimraf": "^2.5.1",
    "s3-cli": "^0.13.0",
    "webpack": "^1.12.14"
  }
}
